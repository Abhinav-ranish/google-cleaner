{
    "manifest_version": 3,
    "name": "Google Search Ad Blocker",
    "version": "1.0",
    "description": "Hides sponsored ads from Google search results.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["https://www.google.com/search*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  